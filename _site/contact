	<script type='text/javascript'>
		$(window).load(function(){
			var img1 = $("img");	
			var img2 = $("<img>").attr("src", "img/slide02.jpg");
			var img3 = $("<img>").attr("src", "img/slide03.jpg");
	
			var slider = $("<div>").attr("id", "slider");
			slider.append(img1).append(img2).append(img3);
			$("#right").append(slider);
			
			$("#slider").nivoSlider({
				effect: 'slideInRight',
				pauseTime:6000,
			});
		});
	</script>
